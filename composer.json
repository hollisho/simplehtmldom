{
  "name": "hollisho/simplehtmldom",
  "type": "library",
  "description": "html dom parse",
  "keywords": [
    "html",
    "dom"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hollis Ho",
      "email": "he_wenzhi@126.com",
      "homepage": "https://blog.1024plus.com",
      "role": "Developer"
    }
  ],
  "require-dev": {
    "phpstan/phpstan": "^0.9.3 || ^1.9 || ^1.10.28",
    "phpunit/phpunit": "^6.5 || ^8.5 || ^9.5"
  },
  "require": {
    "php": "^7.0 || ^8.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "hollisho\\simplehtmldom\\": "src/"
    },
    "files": [
      "src/simple_html_dom.php"
    ],
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "hollisho\\simplehtmldomTests\\": "tests/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  }
}